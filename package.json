    "babel-preset-react": "^6.24.1",
    "csscomb": "^4.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
